Import-Module ActiveDirectory

$DaysInactive = 90
$InactiveDate = (Get-Date).Adddays(-($DaysInactive))

$Users = Get-ADuser -Filter { LastLogonDate -lt $InactiveDate -and Enabled -eq $true } -Properties LastLogonDate | Select-Object Name, LastLogonDate, DistinguishedName

ForEach ($Item in $Users){
    Write-Output $($Item.Name)
}
